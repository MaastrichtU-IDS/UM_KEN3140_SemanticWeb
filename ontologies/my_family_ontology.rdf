<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="https://w3id.org/myonto"
         xmlns="https://w3id.org/myonto#">

<owl:Ontology rdf:about="https://w3id.org/myonto">
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">OWL Ontology to describe a family and its relations.</rdfs:comment>
</owl:Ontology>

<owl:ObjectProperty rdf:about="#is_parent_of">
  <rdfs:domain rdf:resource="#Person"/>
  <rdfs:range rdf:resource="#Person"/>
</owl:ObjectProperty>

<owl:Class rdf:about="#Person">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<Person rdf:about="#Emily">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
</Person>

<Person rdf:about="#Jean">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  <is_parent_of rdf:resource="#Emily"/>
</Person>


</rdf:RDF>
